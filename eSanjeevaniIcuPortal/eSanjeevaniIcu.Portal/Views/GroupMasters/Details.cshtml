@model eSanjeevaniIcu.Portal.Models.GroupMasterViewModel

@{
    ViewData["Title"] = "Group Master";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="PageBorder">
    <div class="row">
        <div class="col-md-12">
            <div class="table color-table PageTitle">
                Group Details
            </div>
        </div>
    </div>
    <div class="table color-table">
        <dl class="row">
            <dt class="col-sm-2 padd5">
                @Html.DisplayNameFor(model => model.GroupName)
            </dt>
            <dd class="col-sm-10 padd5">
                :
                @Html.DisplayFor(model => model.GroupName)
            </dd>
            <dt class="col-sm-2 padd5">
                @Html.DisplayNameFor(model => model.Status)
            </dt>
            <dd class="col-sm-10 padd5">
                :
                @Html.DisplayFor(model => model.StatusName)
            </dd>
        </dl>
        <div class="form-control" style="border:none;">
            <div class="row">
                <div class="col-md-12 text-left">
                    <a asp-action="Edit" asp-route-id="@Model.GroupId" class="btn btn-primary">Edit</a>
                    <a asp-action="Index" class="btn btn-primary">Back to List</a>
                </div>
            </div>
        </div>
        <div class="form-control" style="border:none;">
        </div>
    </div>
</div>
